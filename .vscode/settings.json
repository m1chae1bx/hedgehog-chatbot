{
    "editor.rulers": [
        100
    ],
    "editor.formatOnSave": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length",
        "100"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=100"
    ],
    "cSpell.words": [
        "docstore",
        "FAISS",
        "langchain",
        "pretrained",
        "vectorstores"
    ],
}
